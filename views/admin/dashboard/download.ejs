<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>

    <div class="users-data">

        <% if (users.length> 0) { %>
            <form action="/admin/users/download" method="post">
                <table>
                    <tbody name="pdf">
                        <tr>
                            <th>S/n</th>
                            <th>First Name</th>
                            <th>last Name</th>
                            <th>email</th>
                            <th>phone</th>
                            <th>Acct No</th>
                            <th>Course</th>


                        </tr>

                        <tr>
                            <% users.forEach((user, index)=> { %>
                                <td>
                                    <%= index + 1 %>
                                </td>
                                <td>
                                    <%= user.firstName %>
                                </td>
                                <td>
                                    <%= user.lastName %>
                                </td>
                                <td>
                                    <%= user.email %>
                                </td>
                                <td>
                                    <%= user.phone %>
                                </td>
                                <td>
                                    <%= user.acct_No %>
                                </td>
                                <td>
                                    <%= user.course %>
                                </td>
                                <td>


                                    <% }) %>
                        </tr>
                    </tbody>
                </table>`



                <button type="submit">Download Pdf</button>
            </form>




            <% } %>





    </div>

</body>

</html>